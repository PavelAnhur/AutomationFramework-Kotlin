<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="All UI Test Suite" parallel="classes" thread-count="2">
    <listeners>
        <listener class-name="org.example.core.infra.retry.RetryListener"/>
        <listener class-name="org.example.core.infra.listeners.TestListener"/>
    </listeners>
    <test name="My Store Tests">
        <packages>
            <package name="org.example.tests.ui"/>
        </packages>
    </test>
</suite>